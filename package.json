{
  "name": "fifatables",
  "version": "0.0.4",
  "description": "Library for scraping soccer data from the internet",
  "main": "index.js",
  "scripts": {
    "start": "tsc -w",
    "demo": "ts-node demo",
    "build": "tsc",
    "test": "jest --config test/jest-config.json",
    "copydist": "ts-node --project scripts/tsconfig.ts-node.json scripts/copy-dist",
    "prepare-yaml": "ts-node --project scripts/tsconfig.ts-node.json scripts/prepare-yaml",
    "yaml-to-json": "ts-node --project scripts/tsconfig.ts-node.json scripts/yaml-to-json",
    "postbuild": "yarn copydist",
    "husky:install": "husky install",
    "gpr:setup": "ts-node --project scripts/tsconfig.ts-node.json scripts/gpr-setup",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "postversion": "git push && git push --follow-tags",
    "publish:beta": "yarn && yarn build && npm publish --folder dist --tag beta",
    "release:beta": "npm version prerelease -m \"chore(update): prelease %s β\"",
    "release:patch": "git checkout master && npm version patch -m \"chore(update): patch release %s 🐛 \"",
    "release:minor": "git checkout master && npm version minor -m \"chore(update): release %s 🚀\"",
    "release:major": "git checkout master && npm version major -m \"chore(update): major release %s 💥 \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Celtian/fifatables.git"
  },
  "author": {
    "name": "Dominik Hladík",
    "email": "dominik.hladik@seznam.cz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Celtian/fifatables/issues"
  },
  "homepage": "https://github.com/Celtian/fifatables#readme",
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "byline": "^5.0.0",
    "iconv-lite": "^0.6.3"
  },
  "devDependencies": {
    "@types/byline": "^4.2.33",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^14.17.11",
    "auto-changelog": "^2.3.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4",
    "js-yaml": "^4.1.0"
  },
  "keywords": [
    "data",
    "download",
    "football",
    "scraper",
    "soccer",
    "fifatables"
  ]
}
